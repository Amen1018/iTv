#import "<%= klass.name %>+<%= property.cap_name %>.h"
#import "DKNightVersionManager.h"
<% if klass.all_superklass_name %><% klass.all_superklass_name.reverse.each do |name| %><%= "#import \"#{name}+#{property.name}.h\"\n" %><% end %><% end %>

@implementation <%= klass.name %> (<%= property.cap_name %>)

- (UIColor *)defaultNight<%= property.cap_name %> {
    return [self isMemberOfClass:[<%= klass.name %> class]] ? <% if property.default_color %><%= "UIColorFromRGB(#{property.default_color})" %><% else %><%= 'self.normal' + property.cap_name %><% end %> : nil;
}

@end
